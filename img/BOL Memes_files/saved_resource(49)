        function handle(e) {
            if (typeof e.data === 'undefined' || e.data == null)
                return;
  
            if (typeof e.data != 'string')
                return;
  
            if (e.data.indexOf('dynad-tv') == -1 || status == STATUS.TIMEOUT || status == STATUS.CLOSED)
                return;
  
            if (console && console.log)
                console.log('[dynad-core] - [', Date.now(), '] - ', e.data);
  
            if (e.data.indexOf('dynad-tv-protocol/loaded/') == 0) { // player foi carregado
                try {
                    if (idTimeout != null) {
                        clearTimeout(idTimeout);
                        idTimeout = null;
                    }
                } catch (e) {}
  
                status = STATUS.LOADED;
                if (typeof window.DYNAD_TV.config.container !== 'undefined' && typeof window.DYNAD_TV.config.container.width !== 'undefined') {
                    width = window.DYNAD_TV.config.container.width;
                    height = window.DYNAD_TV.config.container.height;
                } else {
                    var dm = e.data.substr('dynad-tv-protocol/loaded/'.length).split('/');
                    height = parseInt(dm[1]);
                    width = parseInt(dm[0]);
  
                    try {
                        if (height <= 0 || width <= 0) {
                            width = divContainer.parentNode.offsetWidth;
                            height = Math.floor((9 / 16) * width);
                        }
                    } catch (e) {}
                }
  
                try {
                    dispatchEventListener('loaded', {
                        'w': width,
                        'h': height
                    });
                } catch (exEvent) {}
                resize();
                follow();
                notifyMetricsMonitor();
            } else if (e.data.indexOf('dynad-tv-protocol/click/') == 0) { // player nao contem data
                try {
                    if (idTimeout != null) {
                        clearTimeout(idTimeout);
                        idTimeout = null;
                    }
                } catch (e) {}
                try {
                    dispatchEventListener('click');
                } catch (exEvent) {}
                addAdServerCounter(5550001579);
            } else if (e.data == 'dynad-tv-protocol/user_close') {
                container.contentWindow.postMessage('dynad-tv-protocol/force_close_linear', '*');
                window['callbackiruol_user_close'] = function(d) {};
                var e = document.createElement('script');
                e.setAttribute('type', 'text/javascript');
                e.setAttribute('async', 'true');
                e.setAttribute('src', '//metrics.dynad.net/resources/counter/jsonp/multipleIncWithFk/dynadtv/behavior/close_button_floating/override/?callback=callbackiruol_user_close&ord=' + Date.now() + '&override_0=' + encodeURIComponent(idAdSystem + '/' + idAdId + '/' + idCreativeId));
                divContainer.appendChild(e);
            } else if (e.data.indexOf('dynad-tv-protocol/impression_data/') == 0) {
                var op = e.data.substr(('dynad-tv-protocol/impression_data/').length).split('/');
                idAdSystem = op[1], idCreativeId = op[0], idAdId = op[2];
                try {
                    if (op.length > 3) {
                        adVideoDuration = Number(op[3]) * 1000;
                        if (adVideoDuration < 15000)
                            adVideoDuration = 15000
                    }
                } catch (exTimer) {}
            } else if (e.data.indexOf('dynad-tv-protocol/state_skip/') == 0) {
                var op = e.data.substr(('dynad-tv-protocol/state_skip/').length);
                divContainer.setAttribute('state_skip', (op == 'true' ? 'none' : 'block'));
                //var btCloseEle = document.getElementById('floater_bt_close_dynadtv_unique');
                //if( btCloseEle )
                //    btCloseEle.style.display = divContainer.getAttribute('state_skip');                       
                //document.getElementById('floater_bt_close_dynadtv').style.display = (op == 'true' ? 'none' : 'block');****/
            } else if ( e.data.indexOf('dynad-tv-protocol/no_data/') == 0 || e.data == 'dynad-tv-protocol/close' ) { // player nao contem data
                try {
                    if (idTimeout != null) {
                        clearTimeout(idTimeout);
                        idTimeout = null;
                    }
                } catch (e) {}
                if( e.data.indexOf('dynad-tv-protocol/close') == 0 ) {
                    if(!prebidCalled && checkForVideoBids()){
                        prebidCalled = true;
                        //checkForVideoBids();
                        //applyRequiredParams()
                        sendLoadMessage();
                        return;
                    }
                }
                if( e.data.indexOf('dynad-tv-protocol/no_data/') == 0 ) {
                    if(!prebidCalled && checkForVideoBids()){
                        prebidCalled = true;
                        //checkForVideoBids();
                        //applyRequiredParams()
                        sendLoadMessage();
                        return;
                    }else{
                        var m = e.data.substr('dynad-tv-protocol/no_data/'.length).split('/');
                        try {
                            dispatchEventListener('nodata', m);
                        } catch (exEvent) {}
                        //addAdServerCounter(5550001577);
                        notifyMetricsMonitor();
                    }

                }
                
                try {
                    dispatchEventListener('closed');
                } catch (exEvent) {}
  
                if (shadowBox != null)
                    divContainer.style.border = "0px";
                divContainer.style.overflow = "hidden";
  
                divContainer.style.height = '0px';
                divContainer.style.width = '0%';
                divContainer.height = '0px';
  
                if (shadowBox != null)
                    shadowBox.parentNode.removeChild(shadowBox);
  
                setTimeout(function() {
                    container.parentNode.removeChild(container);
                }, 1000);
  
                dispose();
                comScorePauseVideoAdvertisement();
            } else if (e.data == 'dynad-tv-protocol/init') {
                /* Alteracao feita a pedido do Adri para teste */
                comScorePlayVideoAdvertisement();
                if (document.hasFocus() === true || dynadVideoHasMouseIn) {
                    if(window.location.href.indexOf('bol.uol.com.br') > -1) {
                        //checkForVideoBids(function () { 
                            sendLoadMessage();
                        //});
                    } else {
                        sendLoadMessage();
                    }
                } else {
                    try {
                    dispatchEventListener('nodata');
                    } catch (exEvent) {}
  
                    try {
                        if (idTimeout != null) {
                            clearTimeout(idTimeout);
                            idTimeout = null;
                        }
                    } catch (e) {}
  
                    if (shadowBox != null) {
                        divContainer.style.border = "0px";
                    }
                    divContainer.style.overflow = "hidden";
  
                    divContainer.style.height = '0px';
                    divContainer.style.width = '0%';
                    divContainer.height = '0px';
  
                    if (shadowBox != null)
                        shadowBox.parentNode.removeChild(shadowBox);
  
                    setTimeout(function() {
                        container.parentNode.removeChild(container);
                    }, 1000);
  
                    dispose();
                }
            } else if (e.data == 'dynad-tv-protocol/expand') {
                if (typeof window.DYNAD_TV.config.container !== 'undefined' && typeof window.DYNAD_TV.config.container.obj !== 'undefined') {
                        
                    if (window['dynad_skip_overflow_visible'] != true)
                        window.DYNAD_TV.config.container.obj.style.overflow = 'visible';
                }
  
                if (typeof window.DYNAD_TV.config.container !== 'undefined' && typeof window.DYNAD_TV.config.container.width !== 'undefined')
                    divContainer.style.width = window.DYNAD_TV.config.container.width + "px";
                else
                    divContainer.style.width = '100%';
  
                if (typeof window.DYNAD_TV.config.settings !== 'undefined' && window.DYNAD_TV.config.settings['enableShadow'] === 'true') {
                    if (typeof window.DYNAD_TV.config.container !== 'undefined' && typeof window.DYNAD_TV.config.container.obj !== 'undefined') {
                        divContainer.style.display = 'none';
                        var pcW = window.DYNAD_TV.config.container.obj.offsetWidth;
                        var mcW = window.DYNAD_TV.config.container.width;
                        if (pcW > mcW)
                            divContainer.style.marginLeft = Math.round((pcW - mcW) / 2) + "px";
                        divContainer.style.display = 'block';
  
                        var pcH = window.DYNAD_TV.config.container.obj.offsetHeight;
                        var mcH = window.DYNAD_TV.config.container.height;
                        if (pcH > mcH)
                            divContainer.style.marginTop = Math.round((pcH - mcH) / 2) + "px";
  
                        shadowBox = document.createElement('div');
                        shadowBox.setAttribute('style', 'position: absolute; top: 0px; left: 0px; width: ' + pcW + "px; height: " + pcH + "px; background:rgba(1,1,1,0.7); z-index: 2999998;");
                        window.DYNAD_TV.config.container.obj.appendChild(shadowBox);
                        divContainer.style.border = "1px solid white";
                        divContainer.style.overflow = "visible";
                    }
                }
  
                var applyHeight = (vHeight + addMarginOnTop);
                if (typeof window.DYNAD_TV.config.container !== 'undefined' && typeof window.DYNAD_TV.config.container.height !== 'undefined')
                    applyHeight = window.DYNAD_TV.config.container.height;
                else {
  
                }
  
                container.style.height = applyHeight + 'px';
                container.height = applyHeight + 'px';
  
                // Animação desligada em 21/03/2018 para testes de agilidade
                // divContainer.style.animationDuration = "1s";
                // divContainer.style.WebkitTransitionDuration = "1s";
                // divContainer.style.WebkitTransition = "1s";
                // divContainer.style.MozTransition = "1s";
  
  
                divContainer.style.height = applyHeight + 'px';
                divContainer.height = applyHeight + 'px';
                sendStartCounterMessage();
                try {
                    dispatchEventListener('expanded', {
                        'w': vWidth,
                        'h': applyHeight
                    });
                } catch (exEvent) {}
                addAdServerCounter(5550001580);
                if (!comScorePLayDispatched) comScorePlayVideoAdvertisement();
  
                var hasShadowBeingEnabled = false;
                try {
                    hasShadowBeingEnabled = window.DYNAD_TV.config.settings['enableShadow'] == 'true';
                } catch (e) {}
  
                var fontColor = "black";
                if (hasShadowBeingEnabled)
                    fontColor = "white";
  
                var close_button = document.createElement('div');
                close_button.style.setProperty('width', '34px');
                close_button.style.setProperty('height', '34px');
                close_button.style.setProperty('background-position', 'center');
                close_button.style.setProperty('background-repeat', 'no-repeat');
                close_button.style.setProperty('background-size', '24px');
